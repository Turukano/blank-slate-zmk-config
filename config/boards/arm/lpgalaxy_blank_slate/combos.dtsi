combos {
        compatible = "zmk,combos";
        combo_win {
            timeout-ms = <50>;
            key-positions = <14 15>;
            bindings = <&kp LGUI>;
        };

        combo_esc {
            timeout-ms = <50>;
            key-positions = <13 14>;
            bindings = <&kp ESC>;
        };

        combo_btab {
            timeout-ms = <50>;
            key-positions = <8 9>;
            bindings = <&kp LS(TAB)>;
        };

        combo_tab {
            timeout-ms = <50>;
            key-positions = <9 10>;
            bindings = <&kp TAB>;
        };

        combo_switch {
            timeout-ms = <50>;
            key-positions = <20 21 22>;
            bindings = <&kp RA(TAB)>;
        };
    };